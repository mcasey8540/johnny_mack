<table class= "table table-striped table-bordered" >
		<tr>
			<th>Away</th>
			<th>Away Predictor</th>
			<th>Home</th>
			<th>Home Predictor</th>
		<tr>

	<% @games.each do |t| %>
		<tr>
			<td><%= t.away %></td>
			<td><%= t.away_predictor %></td>
			<td><%= t.home %></td>
			<td><%= t.home_predictor %></td>
		<tr>	
	<% end %>
</table>
