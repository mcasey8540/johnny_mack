<%= button_to "New Scrape", new_scrape_path, class: "btn btn-info" %>

<table class= "table table-striped table-bordered" >
		<tr>
			<th>ID</th>
			<th>Date</th>
		<tr>

	<% @scrapes.each do |t| %>
		<tr>
			<td><%= t.id %></td>
			<td><%= link_to t.created_at.to_date, scrape_path(t.id) %></td>
		<tr>	
	<% end %>
</table>


